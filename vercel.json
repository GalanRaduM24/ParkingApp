{
    "version": 2,
    "functions": {
      "api/**/*.php": {
        "runtime": "vercel-php@0.7.3"
      }
    },
    "routes": [
      { "src": "/", "dest": "/php/Home.php" }, 
      { "src": "/css/(.*)", "dest": "/css/$1" }, 
      { "src": "/js/(.*)", "dest": "/js/$1" }    
    ],
    "builds": [
      {
        "src": "api/**/*.php",
        "use": "vercel-php@0.7.3" 
      }
    ]
  }
  