{
    "version": 2,
    "builds": [
        {
          "src": "api/**/*.php",
          "use": "vercel-php@0.6.0" 
        }
      ],
    "routes": [
      { "src": "/", "dest": "/php/Home.php" }, 
      { "src": "/css/(.*)", "dest": "/css/$1" }, 
      { "src": "/js/(.*)", "dest": "/js/$1" }    
    ]
  }
  